  "scripts": {
    "ng": "ng",
    "generate-swagger-api-module": "ng-swagger-gen -c ng-swagger-gen.json",
    "start": "ionic serve",
    "prebuild": "npm run create-version-file",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --watch=false --browsers=\"ChromeHeadless\" --code-coverage",
    "test-watch": "ng test --watch=true --browsers=\"ChromeHeadless\" --reporters=progress",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update-packages": "ncu -u & npm install",
    "create-version-file": "ts-node --project tsconfig.npm-script.json git.version.ts",
    "strip-source-map-url": "ts-node --project tsconfig.npm-script.json strip-source-map-urls.ts",
    "sentry:upload-sourcemaps": "cross-env-shell SENTRY_PROPERTIES=sentry.properties sentry-cli releases files $npm_package_version upload-sourcemaps www -x .js -x .map --validate --verbose --rewrite --strip-common-prefix",
    "sentry:create-release-and-commits": "cross-env-shell SENTRY_PROPERTIES=sentry.properties sentry-cli releases new $npm_package_version && sentry-cli releases set-commits --auto $npm_package_version",
    "sentry:create-release": "cross-env-shell SENTRY_PROPERTIES=sentry.properties sentry-cli releases new $npm_package_version",
    "sentry:finalize-release": "cross-env-shell SENTRY_PROPERTIES=sentry.properties sentry-cli releases finalize $npm_package_version",
    "sentry:create-release-and-upload-sourcemaps": "npm run create-version-file && ng build --prod && npm run sentry:create-release && npm run sentry:upload-sourcemaps && npm run sentry:finalize-release"
  },
