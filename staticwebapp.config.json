{
  "routes": [
    {
      "route": "/ngsw.json",
      "statusCode": 404
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/ngsw.json",
      "/assets/*",
      "/leaflet/*",
      "/svg/*",
      "/turf/*",
      "/*.txt",
      "/*.js",
      "/*.js.map",
      "/*.png",
      "/*.txt",
      "/*.ttf",
      "/*.woff",
      "/*.woff2",
      "/*.otf",
      "/*.css",
      "/*.css.map"
    ]
  }
}
